{
  "name": "snake-typescript",
  "description": "A small project to practice typescript by building a snake game class mixed with webpack bundling.",
  "version": "1.0.0",
  "main": "./dist/snake.js",
  "scripts": {
    "dev": "npm run development",
    "development": "npx webpack --mode=development",
